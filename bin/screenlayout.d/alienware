#!/bin/sh
xrandr --output eDP1 --off --output DP1 --off --output HDMI1 --mode 1920x1080 --pos 3440x0 --rotate left --output HDMI2 --off --output VIRTUAL1 --off --output DP-1-2 --off --output DP-1-3 --off --output HDMI-1-3 --primary --mode 3440x1440 --pos 0x0 --rotate normal --output DVI-I-1-1 --off
